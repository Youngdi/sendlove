    <gap:splash gap:platform="android" gap:qualifier="ldpi" src="www/res/200x320.png" />
    <gap:splash gap:platform="android" gap:qualifier="mdpi" src="www/res/320x480.png" />
    <gap:splash gap:platform="android" gap:qualifier="hdpi" src="www/res/480x800.png" />
    <gap:splash gap:platform="android" gap:qualifier="xhdpi" src="www/res/720x1080.png" />
